package farmApp.Entities;

import java.sql.Timestamp;

/**
 *
 * @author Ghetto
 */
public class Animale {
  
    Amplasarea a = new Amplasarea();
    TipAnimale tip = new TipAnimale();
    
    private int stockId;
    private boolean active;
    private String animalCode;
    private Timestamp deliveryDate;
    private Timestamp receivedDate;
    private String reason;
    private String sellerName;
    private Double receivedWeight;
    private int animalId;
    private int locationId;    

    public Animale() {
    }

    public Animale(boolean active, String animalCode, Timestamp deliveryDate, Timestamp receivedDate, String reason, String sellerName, Double receivedWeight) {
        this.active = active;
        this.animalCode = animalCode;
        this.deliveryDate = deliveryDate;
        this.receivedDate = receivedDate;
        this.reason = reason;
        this.sellerName = sellerName;
        this.receivedWeight = receivedWeight;
    }

    public Animale(int stockId, boolean active, String animalCode, Timestamp deliveryDate, Timestamp receivedDate, String reason, String sellerName, Double receivedWeight) {
        this.stockId = stockId;
        this.active = active;
        this.animalCode = animalCode;
        this.deliveryDate = deliveryDate;
        this.receivedDate = receivedDate;
        this.reason = reason;
        this.sellerName = sellerName;
        this.receivedWeight = receivedWeight;
    }

    public Animale(int stockId, boolean active, String animalCode, Timestamp deliveryDate, Timestamp receivedDate, String reason, String sellerName, Double receivedWeight, int animalId) {
        this.stockId = stockId;
        this.active = active;
        this.animalCode = animalCode;
        this.deliveryDate = deliveryDate;
        this.receivedDate = receivedDate;
        this.reason = reason;
        this.sellerName = sellerName;
        this.receivedWeight = receivedWeight;
        this.animalId = animalId;
    }

    public Animale(int stockId, boolean active, String animalCode, Timestamp deliveryDate, Timestamp receivedDate, String reason, String sellerName, Double receivedWeight, int animalId, int locationId) {
        this.stockId = stockId;
        this.active = active;
        this.animalCode = animalCode;
        this.deliveryDate = deliveryDate;
        this.receivedDate = receivedDate;
        this.reason = reason;
        this.sellerName = sellerName;
        this.receivedWeight = receivedWeight;
        this.animalId = animalId;
        this.locationId = locationId;
    }

    public Animale(Double receivedWeight) {
        throw new UnsupportedOperationException("Not supported yet."); //To change body of generated methods, choose Tools | Templates.
    }

    public int getStockId() {
        return stockId;
    }

    public void setStockId(int stockId) {
        this.stockId = stockId;
    }

    public boolean isActive() {
        return active;
    }

    public void setActive(boolean active) {
        this.active = active;
    }

    public String getAnimalCode() {
        return animalCode;
    }

    public void setAnimalCode(String animalCode) {
        this.animalCode = animalCode;
    }

    public Timestamp getDeliveryDate() {
        return deliveryDate;
    }

    public void setDeliveryDate(Timestamp deliveryDate) {
        this.deliveryDate = deliveryDate;
    }

    public Timestamp getReceivedDate() {
        return receivedDate;
    }

    public void setReceivedDate(Timestamp receivedDate) {
        this.receivedDate = receivedDate;
    }

    public String getReason() {
        return reason;
    }

    public void setReason(String reason) {
        this.reason = reason;
    }

    public String getSellerName() {
        return sellerName;
    }

    public void setSellerName(String sellerName) {
        this.sellerName = sellerName;
    }

    public Double getReceivedWeight() {
        return receivedWeight;
    }

    public void setReceivedWeight(Double receivedWeight) {
        this.receivedWeight = receivedWeight;
    }

    public int getAnimalId() {
        return animalId;
    }

    public void setAnimalId(int animalId) {
        this.animalId = animalId;
    }

    public int getLocationId() {
        return locationId;
    }

    public void setLocationId(int locationId) {
        this.locationId = locationId;
    }

    @Override
    public String toString() {
        return "Animale{" + "a=" + a + ", tip=" + tip + ", stockId=" + stockId + ", active=" + active + ", animalCode=" + animalCode + ", deliveryDate=" + deliveryDate + ", receivedDate=" + receivedDate + ", reason=" + reason + ", sellerName=" + sellerName + ", receivedWeight=" + receivedWeight + ", animalId=" + animalId + ", locationId=" + locationId + '}';
    }

    
    
}
