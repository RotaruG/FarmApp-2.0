package farmApp.Entities;

import java.util.Set;

/**
 *
 * @author Ghetto
 */
public class Amplasarea {
    
    private int id;
    private String building;
    private String cell;
    private String firstDetail;
    private String secondDetail;
    private String sector;
    private String shortLocation;
    private String thirdDetail;
    
    
    
    public Amplasarea() {
    }

    public Amplasarea(int id, String building, String cell, String firstDetail, String secondDetail, String sector, String shortLocation, String thirdDetail) {
        this.id = id;
        this.building = building;
        this.cell = cell;
        this.firstDetail = firstDetail;
        this.secondDetail = secondDetail;
        this.sector = sector;
        this.shortLocation = shortLocation;
        this.thirdDetail = thirdDetail;
        
    }

    
    public Amplasarea(String building, String cell, String firtsDetail, String secondDetail, String sector, String shortLocation, String thirdDetail) {
        this.building = building;
        this.cell = cell;
        this.firstDetail = firtsDetail;
        this.secondDetail = secondDetail;
        this.sector = sector;
        this.shortLocation = shortLocation;
        this.thirdDetail = thirdDetail;
    }

   

    public String getThirdDetail() {
        return thirdDetail;
    }

    public void setThirdDetail(String thirdDetail) {
        this.thirdDetail = thirdDetail;
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getBuilding() {
        return building;
    }

    public void setBuilding(String building) {
        this.building = building;
    }

    public String getCell() {
        return cell;
    }

    public void setCell(String cell) {
        this.cell = cell;
    }

    public String getFirstDetail() {
        return firstDetail;
    }

    public void setFirstDetail(String firstDetail) {
        this.firstDetail = firstDetail;
    }

    public String getSecondDetail() {
        return secondDetail;
    }

    public void setSecondDetail(String secondDetail) {
        this.secondDetail = secondDetail;
    }

    public String getSector() {
        return sector;
    }

    public void setSector(String sector) {
        this.sector = sector;
    }

    public String getShortLocation() {
        return shortLocation;
    }

    public void setShortLocation(String shortLocation) {
        this.shortLocation = shortLocation;
    }

    @Override
    public String toString() {
        return shortLocation;
    }

   
    
    
    
    
    
}
